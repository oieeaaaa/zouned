{
  "name": "zouned",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./api/index.js",
    "dev": "nodemon ./api/index.js",
    "build": "next build",
    "fix:eslint": "eslint --fix ./ --ext .js --ext .jsx",
    "fix:stylelint": "stylelint --fix ./**/*.{css,scss,sass}",
    "fix": "npm run fix:eslint && npm run fix:stylelint",
    "component": "plop component",
    "db:init": "sequelize db:migrate && sequelize db:seed:all",
    "db:clean": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "flickity": "^2.2.1",
    "formidable": "^1.2.2",
    "lodash.has": "^4.5.2",
    "lodash.throttle": "^4.1.1",
    "music-metadata": "^6.4.0",
    "mysql2": "^2.1.0",
    "next": "9.3.2",
    "node-fetch": "^2.6.0",
    "react": "16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "16.13.1",
    "react-flickity-component": "^3.5.0",
    "react-svg": "^11.0.21",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
